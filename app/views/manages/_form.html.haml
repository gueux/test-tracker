= form_for @manage do |f|
  - if @manage.errors.any?
    #error_explanation
      %h2= "#{pluralize(@manage.errors.count, "error")} prohibited this manage from being saved:"
      %ul
        - @manage.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :index
    = f.text_field :index
  .actions
    = f.submit 'Save'
